[[collectd]]
  enabled = true
  bind-address = ":{{ INFLUXDBPORT }}" 
  database = "{{ COLLECTD_DB_NAME }}" 
  retention-policy = ""
  batch-size = 5000 
  batch-pending = 10 
  batch-timeout = "10s"
  read-buffer = 0 
  typesdb = "/usr/share/collectd/types.db"

